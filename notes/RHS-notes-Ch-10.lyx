\end_layout

\begin_deeper
\begin_layout Standard
\begin_inset Formula 
\[
y^{*}=X_{i}\beta+\epsilon_{i}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
y=\begin{cases}
1 & \mathrm{if}\,y*>0\\
0 & y*\le0
\end{cases}
\end{equation}

\end_inset


